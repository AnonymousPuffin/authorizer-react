    "@storybook/test": "^8.2.7",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "babel-loader": "^9.1.0",
    "husky": "^8.0.2",
